<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.hearoad.hearoad.ui.fragment.HomeFragment"
        android:label="home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_mypage"
        android:name="com.hearoad.hearoad.ui.fragment.MypageFragment"
        android:label="mypage"
        tools:layout="@layout/fragment_mypage" />

    <fragment
        android:id="@+id/navigation_sound"
        android:name="com.hearoad.hearoad.ui.fragment.SoundFragment"
        android:label="sound"
        tools:layout="@layout/fragment_sound" />
</navigation>